<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_product_label_tree" model="ir.ui.view">
        <field name="name">product.label.tree</field>
        <field name="model">product.label</field>
        <field name="arch" type="xml">
            <tree string="Etiquetas de Productos">
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="action_product_label" model="ir.actions.act_window">
        <field name="name">Etiquetas de Productos</field>
        <field name="res_model">product.label</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Crea una nueva etiqueta.</p>
        </field>
    </record>

    
</odoo>